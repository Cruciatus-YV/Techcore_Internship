{
  "Zipkin": {
    "Endpoint": "http://zipkin:9411/api/v2/spans"
  },
  "OpenTelemetry": {
    "ServiceName": "GrpcServiceA",
    "ServiceVersion": "1.0.0"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Grafana.Loki" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQ": {
    "Host": "rabbitmq"
  }
}